  "crun get containers": {
    "body": [
      "```sh",
      "oc -n ${NAMESPACE} get pod ${POD}  -ojson | jq -r '.spec.containers | .[] | .name'",
      "```"
    ],
    "prefix": "crun get containers"
  },
  "crun get event for": {
    "body": [
      "```sh",
      "oc -n ${NAMESPACE} get events --field-selector involvedObject.name=${NAME} --sort-by .lastTimestamp",
      "```"
    ],
    "prefix": "crun get event for"
  },
  "crun get ovn master logs": {
    "body": [
      "```sh",
      "for pod in $(oc -n openshift-ovn-kubernetes get pods -l app=ovnkube-master -o custom-columns=:.metadata.name --no-headers); do",
      "  oc -n openshift-ovn-kubernetes logs \"${pod}\" --tail 50 -c ovnkube-master | grep -v \"lock is held by\" | grep -v \"failed to acquire lease\"",
      "done",
      "```"
    ],
    "prefix": "crun get ovn master logs"
  },
